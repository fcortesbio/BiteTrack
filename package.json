  "keywords": [
    "api",
    "food",
    "business",
    "express",
    "mongodb"
  ],
  "homepage": "https://github.com/fcortesbio/BiteTrack#readme",
  "bugs": {
    "url": "https://github.com/fcortesbio/BiteTrack/issues"
  },
  "repository": {
